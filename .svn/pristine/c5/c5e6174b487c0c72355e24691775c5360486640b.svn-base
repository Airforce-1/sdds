	//部门对话框
	$("#departDialog").dialog({
		autoOpen: false,
		modal: true,
		width: 280,
		height: 420,
		position: ["center",20],
		buttons: {
			"OK": function() {
				$("#sddscpJgdw").val($("input[name='departId']:checked").val());
				$("#sddscpJgdwname").val($("input[name='departId']:checked+span").html());
				$("#s1").hide();
				$("#s2").hide();
				$("#s3").hide();
				$(this).dialog("close");
			}
		}
	});
	$("#departDialog1").dialog({
		autoOpen: false,
