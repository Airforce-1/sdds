			}
    	}
    	Map<String,String> tjMap = new HashMap<String, String>();
    	tjMap.put("deptYqhjSum", deptYqhjSum + "");
    	tjMap.put("jgdwYqhjDept", jgdwYqhjDept + "");
    	tjMap.put("dzzYqhjDept", dzzYqhjDept + "");
    	tjMap.put("zbYqhjDept", zbYqhjDept + "");
    	return tjMap;
	}

