							if(!cmsDepartment.getSddspoOrgtype().equals("党总支")){
								list.add(cmsDepartment);
							}
						}
						*/
						list.add(cmsDepartment);
					}
				}
			}
		}
		//List<CmsDepartment> list=dao.getList(parentId,all);
		return list;
	}
	
