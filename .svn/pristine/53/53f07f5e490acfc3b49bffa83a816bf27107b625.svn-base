package com.risen.entity;


public class RisenQairesItem {
	private Integer id;
	private RisenQairesTopic qairesTopic;
	private String itemTitle;
	private Integer itemCount;
	private Integer priority;
	private String itemOption;
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public RisenQairesTopic getQairesTopic() {
		return qairesTopic;
	}
	public void setQairesTopic(RisenQairesTopic qairesTopic) {
		this.qairesTopic = qairesTopic;
	}
	public String getItemTitle() {
		return itemTitle;
	}
	public void setItemTitle(String itemTitle) {
		this.itemTitle = itemTitle;
	}
	public Integer getItemCount() {
		return itemCount;
	}
	public void setItemCount(Integer itemCount) {
		this.itemCount = itemCount;
	}
	public Integer getPriority() {
		return priority;
	}
	public void setPriority(Integer priority) {
		this.priority = priority;
	}
	public String getItemOption() {
		return itemOption;
	}
	public void setItemOption(String itemOption) {
		this.itemOption = itemOption;
	}
	
	
}
