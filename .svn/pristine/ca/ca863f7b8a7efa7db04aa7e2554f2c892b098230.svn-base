    	function sitesnav(pid){
    		$.ajax({  
		         type : "post",  
		          url : "getSites.jspx?pid="+pid,  
		          data : {},
		          dataType : "json" , 
		          success : function(data){
			        $("#sr_dj").html("");
			        if(data.length>0){
			        	for(var i=0;i<data.length;i++){
			          		$("#sr_dj").append("<a title="+data[i].name+" href='javascript:sitesnav2("+data[i].id+")'>"+data[i].name+"</a>");
			        	}
			        }
		          }
		     });
    	}
    	function sitesnav2(pid){
    		$.ajax({  
		         type : "post",  
		          url : "getSites.jspx?pid="+pid,  
		          data : {},
		          dataType : "json" , 
		          success : function(data){
			        $("#jd").html("");
			        if(data.length>0){
			        	$(".neir").css({"display":"none"});
			        	$(".jied").show();
			        	for(var i=0;i<data.length;i++){
			          		$("#jd").append("<a href=''>"+data[i].name+"</a>");
			        	}
			        }
		          }
		     });
    	}
