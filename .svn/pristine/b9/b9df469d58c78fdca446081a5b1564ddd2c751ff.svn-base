<script type="text/javascript">
$(function(){
	Cms.leftLi();
	$("li").each(function(i){
		$(this).removeClass("leftCurr");
		if(i==0){
			$(this).addClass("leftCurr");
		}else{
			$(this).addClass("leftNol");
		}
		$(this).click( function () {
			$("li").each(function(){
				$(this).removeClass("leftCurr");
				$(this).addClass("leftNol");
			});
			$(".h3").each(function(){
				$(this).next().hide();
				$(this).removeClass("leftCurr");
				$(this).addClass("leftNol");
			});
			$(this).removeClass("leftNol");
			$(this).addClass("leftCurr");
		});
	});
	$(".h3").each(function(){
		$(this).next().hide();
		$(this).addClass("leftNol");
		$(this).click( function () {
			$(".h3").each(function(){
				$(this).next().hide();
				$(this).removeClass("leftCurr");
				$(this).addClass("leftNol");
			});
			$("li").each(function(){
				$(this).removeClass("leftCurr");
				$(this).addClass("leftNol");
			});
			$(this).removeClass("leftNol");
			$(this).addClass("leftCurr");
		    $(this).next().show(); 
		});
	});
	$(".h3-child div").each(function(i){
		$(this).click( function () {
			$(".h3-child div").each(function(){
				$(this).removeClass();
				$(this).addClass("leftmenuBG");
			});
			$(this).removeClass();
			$(this).addClass("leftmenuBGCurr");
		});
	});
});
</script>
