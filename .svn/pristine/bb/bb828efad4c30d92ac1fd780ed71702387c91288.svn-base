	$(function(){ 
	$(".dropdown .input_select").click(function(){ 
	var ul = $(this).next('ul'); 
	if(ul.css("display")=="none"){ 
	ul.slideDown("fast"); 
	}else{ 
	ul.slideUp("fast"); 
	} 
	}); 
	$(".dropdown ul li a").click(function(){ 
	var txt = $(this).text(); 
	$(this).parents('ul').prev('.input_select').val(txt); 
	var value = $(this).attr("rel"); 
	$(this).parents('ul').hide(); 
	}); 
	}); 
</script> 
