<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
<title></title>
<#include "/jeecms_sys/head.html"/>
<script type="text/javascript">
function getTableForm() {
	return document.getElementById('tableForm');
}
function optDelete() {
	if(Pn.checkedCount('ids')<=0) {
		$.alert("<@s.m 'global.prompt'/>","<@s.m 'error.checkRecord'/>");
		return;
	}
	$.confirm(doDelete,"<@s.m 'global.confirm'/>","<@s.m 'global.confirm.delete'/>");
}
function doDelete(){
	var f = getTableForm();
	f.action="o_delete.do";
	f.submit();
}
</script>
</head>
<body>
<div class="page-positon ove">
  <div class="position-div"><@s.m "global.position"/>: 目标管理 > 我的目标</div>
</div>
<div class="box-positon ck">
	<form class="ropt">
		<input type="submit" value="<@s.m "global.backToList"/>" onclick="this.form.action='../risenTarget/v_mylist.do';" class="return-button"/>
	</form>
	<div class="clear"></div>
</div>
<div class="body-box">
<form id="tableForm" method="post">
<input type="hidden" name="pageNo" value="${pageNo!}"/>
<@p.table value=pagination;RisenTargetDetail,i,has_next><#rt/>
	<@p.column title="<input type='checkbox' onclick='Pn.checkbox(\"ids\",this.checked)'/>" width="20">
		<input type='checkbox' name='ids' value='${RisenTargetDetail.id}'/><#t/>
	</@p.column><#t/>
	<@p.column title="ID" align="center">${RisenTargetDetail.id}</@p.column><#t/>
	<@p.column code="党组织名称" align="center">${RisenTargetDetail.risentgdOrgname!}</@p.column><#t/>
	<@p.column code="内容" align="center">${RisenTargetDetail.risentgdContent!}</@p.column><#t/>
	<@p.column code="数量得分" align="center">${RisenTargetDetail.risentgdNumscore!'0'}</@p.column><#t/>
	<@p.column code="质量得分" align="center">${RisenTargetDetail.risentgdQualityscore!'0'}</@p.column><#t/>
	<@p.column code="状态" align="center"><#if RisenTargetDetail.risentgdStatus?? && RisenTargetDetail.risentgdStatus=="0">未开始
	<#elseif RisenTargetDetail.risentgdStatus?? && RisenTargetDetail.risentgdStatus=="1">进行中
	<#elseif RisenTargetDetail.risentgdStatus?? && RisenTargetDetail.risentgdStatus=="9">已完成<#else></#if></@p.column><#t/>
	<#t/>
</@p.table>
</form>
</div>
<#include "/common/alert_message.html"/>
</body>
</html>