	$(objDiv).css("display", "none"); 
} 

//动态给my97设置选中颜色
function attrClass(obj,type,day){
	if (type=='1') {
		$(obj).attr("class","Wselday_dydh");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_dydh");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
			show(obj,day,"showTitle","党员大会");
		});
	}else if (type=='2') {
		$(obj).attr("class","Wselday_dxzhy");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_dxzhy");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
			show(obj,day,"showTitle","党小组会议");
		});
	}else if (type=='3') {
		$(obj).attr("class","Wselday_dk");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_dk");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
		  	show(obj,day,"showTitle","党课会议");
		});
	}else if (type=='4') {
		$(obj).attr("class","Wselday_dzbwyh");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_dzbwyh");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
		  	show(obj,day,"showTitle","党支部委员会");
		});
	}else if (type=='5') {
		$(obj).attr("class","Wselday_dzbzzshh");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_dzbzzshh");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
		  	show(obj,day,"showTitle","党支部组织生活会");
		});
	}else if (type=='6') {
		$(obj).attr("class","Wselday_ztdr");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday_ztdr");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
		  	show(obj,day,"showTitle","主题党日活动");
		});
	}else if(type=='000'){
		$(obj).attr("class","Wselday");
		$(obj).mouseout(function(){
		  	$(obj).attr("class","Wselday");
		  	hide(obj,"showTitle");
		});
		$(obj).mouseover(function(){
		  	show(obj,day,"showTitle","暂无活动开展");
		});
	}
}

