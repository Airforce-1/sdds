function addStyle(className,haveDay,type){
	var nowDate = new Date().getDate();
	$("#wd_iframe").contents().find(className).each(function(){
		var day = $(this).html();
		if(day == nowDate){
			$(this).attr("class","Wday");
		}
		if (day==parseInt(haveDay)) {
			attrClass(this,type,day);
		}
	});
}
function removeNowDateStyle(className,haveDay){
	var nowDate = new Date().getDate();
	$("#wd_iframe").contents().find(className).each(function(){
		var day = $(this).html();
		if(day == nowDate){
			$(this).attr("class","Wday");
		}
	});
}
function getByMonthAll(){
	var nowDate = new Date().getDate();
	removeNowDateStyle(".Wday",nowDate);
	removeNowDateStyle(".Wwday",nowDate);
	removeNowDateStyle(".Wselday",nowDate);
	[#if rolcByMonthList??]
		[#list rolcByMonthList as rolcByMonth]
			var time = '${rolcByMonth.risenlcOdate?string("yyyy-MM-dd")}';
			var type = '${rolcByMonth.risenlcMeetingtype!}';
			var timearr=time.replace(" ",":").replace(/\:/g,"-").split("-");
			$("#wd_iframe").contents().find(".Wday").each(function(){
				var day = $(this).html();
				if (day==parseInt(timearr[2])) {
					attrClass(this,type,day);
				}
			});
			
			$("#wd_iframe").contents().find(".Wwday").each(function(){
				var day = $(this).html();
				if (day==parseInt(timearr[2])) {
					attrClass(this,type,day);
				}
			});
			
			$("#wd_iframe").contents().find(".Wselday").each(function(){
				var day = $(this).html();
				if (day==parseInt(timearr[2])) {
					attrClass(this,type,day);
				}else{
					attrClass(this,'000',day);
				}
				
			});
		[/#list]
	[/#if]
}

