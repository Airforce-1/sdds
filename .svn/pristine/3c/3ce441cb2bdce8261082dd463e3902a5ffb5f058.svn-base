<script type="text/javascript">
function showXfph(id){
	$("#alert4").show();
	$("#xfphul li:not(#firstLine)").empty();
	$.ajax({  
		type : "post",  
		url : "${base}/findXfpx.jspx?departid="+id,  
		data : {},
		dataType : "json" , 
		success : function(data){
			if(data.length>0){
				for(var i=0;i<data.length;i++){
					var datas = data[i].split(',');
					var username = datas[0];
					var deptName = datas[1];
					var qtScore = datas[2];
					
			        $("#xfphul").append("<li><span style='margin:0 20px;'>"+username+"</span><span style='margin:0 20px;'>"+deptName+"</span><span style='margin:0 10px;'>"+qtScore+"</span></li>");
				}
			}
		}
	});
}
