function checkType(type){
	var t='';
	if(type=="1"){
		t="支部党员大会";
	}else if(type=="2"){
		t="支部委员会";
	}else if(type=="3"){
		t="党小组会";
	}else if(type=="4"){
		t="党课";
	}else if(type=="5"){
		t="组织生活会";
	}else if(type=="6"){
		t="民主评议";
	}else if(type=="7"){
		t="主题党日";
	}
	return t;
}
