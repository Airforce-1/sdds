			if(!(data.risenlcLd3==null || data.risenlcLd3=="" || typeof(data.risenlcLd3)=="undefined")&&(data.risenlcYn3=="1")){
				fifthLine = fifthLine + "," + data.risenlcLd3;
			}
			if(!(data.risenlcLd4==null || data.risenlcLd4=="" || typeof(data.risenlcLd4)=="undefined")&&(data.risenlcYn4=="1")){
				fifthLine = fifthLine + "," + data.risenlcLd4;
			}
			if(!(data.risenlcLd5==null || data.risenlcLd5=="" || typeof(data.risenlcLd5)=="undefined")&&(data.risenlcYn5=="1")){
				fifthLine = fifthLine + "," + data.risenlcLd5 ;
			}
			if(!(data.risenlcLd6==null || data.risenlcLd6=="" || typeof(data.risenlcLd6)=="undefined")&&(data.risenlcYn6=="1")){
				fifthLine = fifthLine + "," + data.risenlcLd6;
			}
			fifthLine = fifthLine + "</td></tr>";
			$("#data1").append(fifthLine);
			if(data.risenlcComment==null || data.risenlcComment=="" || typeof(data.risenlcComment)=="undefined"){
				var sixthLine = "<tr><td class='rights'>描　　　述：</td><td colspan='3'><textarea rows='3' disabled cols='60'>未填写</textarea></td></tr>";
				$("#data1").append(sixthLine);
			}else{
				var reg=new RegExp('<p>','g'); //创建正则RegExp对象  
				var reg1=new RegExp('</p>','g'); //创建正则RegExp对象  
				var newstr=data.risenlcComment.replace(reg,'');  
				var content=newstr.replace(reg1,'\r\n');  
				var sixthLine = "<tr><td class='rights'>描　　　述：</td><td colspan='3'><textarea rows='3' disabled cols='60'>"+content+"</textarea></td></tr>";
				$("#data1").append(sixthLine);
			}
			var seventhLine = "<tr style='text-align:center;'><td colspan='4'><input type='button' class='btn' name='print' onClick='printMe("+id+");' value='打印'/>";
			seventhLine = seventhLine + "<input class='btn' type='button' name='close' style='margin-left:10px;' onClick='closeMe();' value='关闭'/></td></tr>";
