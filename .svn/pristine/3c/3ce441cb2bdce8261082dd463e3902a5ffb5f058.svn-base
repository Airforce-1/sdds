function inParty(){
	var risendpName = $("[name='risendpName']").val();
	var risendpIdnumber = $("[name='risendpIdnumber']").val();
	var risendpBirth = $("[name='risendpBirth']").val();
	var risendpNation = $("[name='risendpNation']").val();
	var risendpNative = $("[name='risendpNative']").val();
	
	if(risendpName==""){
		alert("请输入姓名");
		return false;
	}else if(risendpIdnumber==""){
		alert("请输入身份证号码");
		return false;
	}else if(risendpIdnumber.length!=18){
		alert("身份证号码长度为18位");
		return false;
	}else if(risendpBirth==""){
		alert("请输入生日");
		return false;
	}else if(risendpNation==""){
		alert("请输入民族");
		return false;
	}else if(risendpNative==""){
		alert("请输入籍贯");
		return false;
	}else{
		var url = "${base}/risenDevparty/save.jspx";
		var risendpExpands1 = $("[name='risendpExpands1']").val();
		var risendpExpands3 = $("[name='risendpExpands3']").val();
		var risendpMarriage = $("[name='risendpMarriage']:checked").val();
		var risendpSex = $("[name='risendpSex']:checked").val();
		var risendpBranch = $("[name='risendpBranch']").val();
		$.ajaxSetup({
       	 contentType: "application/x-www-form-urlencoded; charset=utf-8"
         });
		$.post(url,{risendpMarriage:risendpMarriage,risendpSex:risendpSex,risendpBranch:risendpBranch,risendpExpands3:risendpExpands3,risendpName:risendpName,risendpIdnumber:risendpIdnumber,risendpBirth:risendpBirth,risendpNation:risendpNation,risendpNative:risendpNative,risendpExpands1:risendpExpands1},function(result){
			if(result=="success"){
				alert("添加成功");
				$("#alert5").hide();
			}else{
				alert("添加失败,请稍候添加");
			}
		});
		return true;
	}
}
function ScrollImgLeft_toDay(){ 
	var speed_toDay=50; 
	var scroll_begin_toDay = document.getElementById("scroll_begin_toDay"); 
	var scroll_end_toDay = document.getElementById("scroll_end_toDay"); 
	var scroll_div_toDay = document.getElementById("scroll_div_toDay"); 
	scroll_end_toDay.innerHTML=scroll_begin_toDay.innerHTML; 
	function Marquee(){ 
	if(scroll_end_toDay.offsetWidth-scroll_div_toDay.scrollLeft<=0) 
		scroll_div_toDay.scrollLeft-=scroll_begin_toDay.offsetWidth; 
	else 
		scroll_div_toDay.scrollLeft++; 
	} 
	var MyMar_toDay=setInterval(Marquee,speed_toDay); 
	scroll_div_toDay.onmouseover=function() {clearInterval(MyMar_toDay);} 
	scroll_div_toDay.onmouseout=function() {MyMar_toDay=setInterval(Marquee,speed_toDay);} 
} 

function ScrollImgLeft_toMonth(){ 
	var speed=50; 
	var scroll_begin = document.getElementById("scroll_begin_toMonth"); 
	var scroll_end = document.getElementById("scroll_end_toMonth"); 
	var scroll_div = document.getElementById("scroll_div_toMonth"); 
	scroll_end.innerHTML=scroll_begin.innerHTML; 
	function Marquee(){ 
	if(scroll_end.offsetWidth-scroll_div.scrollLeft<=0) 
		scroll_div.scrollLeft-=scroll_begin.offsetWidth; 
	else 
		scroll_div.scrollLeft++; 
	} 
	var MyMar=setInterval(Marquee,speed); 
	scroll_div.onmouseover=function() {clearInterval(MyMar);} 
	scroll_div.onmouseout=function() {MyMar=setInterval(Marquee,speed);} 
} 

function getToday(date){
	var year = date.split("-")[0];
	var month = date.split("-")[1];
	var url = "${base}/changeStyle.jspx";
	$.ajax({  
		type : "post",  
		url : url,
		data : {departId:did,year:year,month:month},
		dataType : "json" , 
		success : function(data){
			if(data.length>0){
				for(var i = 0;i<data.length;i++){
					var datas = data[i].split(',');
					var day = datas[0];
					var type = datas[1];
					addStyle(".Wday",day,type);
					addStyle(".Wwday",day,type);
					addStyle(".Wselday",day,type);
				}
			}else{
				removeNowDateStyle(".Wday",date);
				removeNowDateStyle(".Wwday",date);
				removeNowDateStyle(".Wselday",date);
			}
		}
	});
	$("#dataInfo").html("");
	$.post('${base}/getAll.jspx',{"date":date,"id":did},function(data){
		var t="";
		if(data.length<1){
			$("#dataInfo").append("<li>暂无活动</li>");
		}else{
			for(var i=0;i<data.length;i++){
				if("8"==data[i].risenlcMeetingtype){
					var con=Format(data[i].risenlcOdate,false)+"支部【"+data[i].risenlcOrgname+"】换届，请及时处理";
					$("#dataInfo").append("<li><a href='javascript:;' style='width:200px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis' title='"+con+"' onClick=''>"+con+"</a></li>");
				}else{
					var content = "";
					t=checkType(data[i].risenlcMeetingtype);
					if((data[i].risenlcAddress==null || data[i].risenlcAddress=="" || typeof(data[i].risenlcAddress)=="undefined") &&  
						(data[i].risenlcHoldername==null || data[i].risenlcHoldername=="" ||  typeof(data[i].risenlcHoldername)=="undefined")){
						content=Format(data[i].risenlcOdate,false)+":开展"+t;
					}else if(data[i].risenlcAddress==null || data[i].risenlcAddress=="" || typeof(data[i].risenlcAddress)=="undefined"){
						content=Format(data[i].risenlcOdate,false)+":开展由"+data[i].risenlcHoldername+"组织的"+t;
					}else if(data[i].risenlcHoldername==null || data[i].risenlcHoldername=="" ||  typeof(data[i].risenlcHoldername)=="undefined"){
						content=Format(data[i].risenlcOdate,false)+":在"+data[i].risenlcAddress+"开展"+t;
					}else{
						content=Format(data[i].risenlcOdate,false)+":在"+data[i].risenlcAddress+"开展由"+data[i].risenlcHoldername+"组织的"+t;
					}
					$("#dataInfo").append("<li><a href='javascript:;' style='width:500px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis' title='"+content+"' onClick='showCaInfo("+data[i].risenlcUuid+");'>"+content+"</a></li>");
				}
			}
		}
	});
}	

