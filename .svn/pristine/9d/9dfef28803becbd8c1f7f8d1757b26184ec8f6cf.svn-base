/**大事记**/
.article_dsj{width:900px; float: left; height:660px;  overflow-y:scroll; margin:10px; margin-left:20px; margin-right:20px; margin-top:8px;}
.cente{width:1240px; margin:0 auto; height:100%; position: relative;}
.cente .con_le{width:150px; float: left; overflow: hidden; min-height:200px; display: block; background:url(../imagds/2.png) repeat-y center;  margin-top:40px; text-align: center; line-height:50px; font-size:14px; color:#fff;    }
.cente #die{width:100px; background:#d53340; position: absolute; bottom:0px; left:22px; height:30px; line-height:30px; color:#fff;       }
.cente .con_le span{display: block; width:52px; height:52px; background:url(../imagds/1.png) no-repeat; margin-left:50px;     float: left;}
.cente .con_ri{width:1240px; position: absolute; top:0px; right: 30px;  margin-top:30px; }
.cente .con_ri .kuae{width:1040px;  float: right; height:60px; border-bottom:solid 2px #f45656;   }
.cente .con_ri .daoh{width:950px; height:40px; float: right; overflow: hidden; position: relative;}
.cente .con_ri .daoh ul{width:950px; border-radius:5px;  float:left;  height:40px; position: absolute; left:0px; }
.cente .con_ri .daoh li{float: left; width:160px; height:40px; line-height:40px; font-size:14px; background:url(../imagds/time1.png) 100% 0  no-repeat;  text-align: center; margin-right:35px; }
.cente .con_ri .daoh li a{color:#fff; display: block; }
.cente .con_ri .daoh li.on{background:url(../imagds/time2.png) 100% 0  no-repeat; border-radius:5px;  }
.cente .con_ri .shig{float: right; width:1210px; margin-top:20px; }
.cente .con_ri .shig li{width:100%; background:url(../imagds/4.png) no-repeat 72px 50%; margin-top:15px;  min-height:50px;    overflow: hidden; }
.cente .con_ri .shig li span{width:60px; height:15px; color:#fff; background:#f45656; padding: 2px 5px;  }
.cente .con_ri .shig li .month{width:1030px; float: right;}
.cente .con_ri .shig li .Cdi{width:1030px; float: right; background:#f3f3f3; border:solid 1px #e8e8e8; line-height:20px; padding:5px; margin-top:5px;  }

#prev{width:40px; margin-top:2px;  height:36px; background:url(../imagds/prev.png) no-repeat; display: block; float: left;}
#next{width:40px; margin-top:2px;  height:36px; background:url(../imagds/next.png) no-repeat 100% 0; display: block; float: right;}
.artit{width:100%; height:30px; text-align: center; font-size:20px; line-height:30px; margin-top:10px;    }
.artit span{color:#bb976d; font-weight: bold;}

.Electronic{padding:0 10px; font-size:14px; line-height:30px; margin-top:10px;  }






