package com.risen.entity;

public class RisenIntegral {
	private Integer risenitUuid;//编号
	private Integer risenitOrgid;//组织编号
	private String risenitOrgname;//组织名称
	private Integer risenitScore;//分数
	private Integer risenitBase;//加分基数
	
	
	public Integer getRisenitBase() {
		return risenitBase;
	}
	public void setRisenitBase(Integer risenitBase) {
		this.risenitBase = risenitBase;
	}
	public Integer getRisenitUuid() {
		return risenitUuid;
	}
	public void setRisenitUuid(Integer risenitUuid) {
		this.risenitUuid = risenitUuid;
	}
	public Integer getRisenitOrgid() {
		return risenitOrgid;
	}
	public void setRisenitOrgid(Integer risenitOrgid) {
		this.risenitOrgid = risenitOrgid;
	}
	public String getRisenitOrgname() {
		return risenitOrgname;
	}
	public void setRisenitOrgname(String risenitOrgname) {
		this.risenitOrgname = risenitOrgname;
	}
	public Integer getRisenitScore() {
		return risenitScore;
	}
	public void setRisenitScore(Integer risenitScore) {
		this.risenitScore = risenitScore;
	}
	
	
}
