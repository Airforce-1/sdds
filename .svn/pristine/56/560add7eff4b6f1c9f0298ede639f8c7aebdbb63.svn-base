    <script type="text/javascript"> 
	var names=[];
	var scores=[];
	$(document).ready(function(){
     		var urltj = "${base}/getjxtj.jspx?deptId=${(channel.depart.id)!}&root=1";
    		$.ajax({  
		         type : "post",  
		          url : urltj,  
		          async : false,  
		          success : function(result){ 
					for(var i=0;i<result.length;i++){
					names[i]=result[i].risenitOrgname;
					scores[i]=result[i].risenitScore;
					}
					
		          }
			});
    		 var myChart = echarts.init(document.getElementById('jxtj1'));
    		 // 指定图表的配置项和数据
		    var option = {
			    title: {
			        x: 'center',
			        text: '党建绩效积分统计'
			       
			    },
			    tooltip: {
			        trigger: 'item'
			    },
			    toolbox: {
			        show: true,
			        feature: {
			            dataView: {show: true, readOnly: true},
			            restore: {show: true},
			            saveAsImage: {show: true}
			        }
			    },
			    calculable: true,
			    grid: {
			        borderWidth: 0,
			        y: 80,
			        y2: 60
			    },
			    grid:{bottom:150},
			    xAxis: [
			        {
			            type: 'category',
			           
			            data:names,
			             
			            axisLabel:{
			              interval:0,  //类目全显
			              formatter:function(val){
			              //debugger;
			              	var s=val.substring(0,val.length/2);
			              	var s1=val.substring(val.length/2,val.length);
						    return s+"\n"+s1;
						    },
			              rotate:-45   //顺时针旋转45度
			            }
			        }
			    ],
			    yAxis: [
			        {
			            type: 'value'
			        }
			    ],
			    series: [
			        {
			            name: '党建绩效积分统计',
			            type: 'bar',
			            itemStyle: {
			                normal: {
			                    color: function(params) {
			                        // build a color map as your need.
			                        var colorList = [
			                          '#C1232B','#B5C334','#FCCE10','#E87C25','#27727B',
			                           '#FE8463','#9BCA63','#FAD860','#F3A43B','#60C0DD',
			                           '#D7504B','#C6E579','#F4E001','#F0805A','#26C0C0'
			                        ];
			                        return colorList[params.dataIndex]
			                    },
			                    label: {
			                        show: true,
			                        position: 'top'
			                    }
			                }
			            },
			            data: scores,
			            markPoint: {
			                tooltip: {
			                    trigger: 'item'
			                },
			                 data : [
			                    {type : 'max', name: '最高积分'},
			                    {type : 'min', name: '最低积分'}
			                ]
			            }
			        }
			    ]
		    };

        // 使用刚指定的配置项和数据显示图表。
        myChart.setOption(option);   
        
		$('.carousel').carousel({
			directionNav:true,
			speed:500, 
			carouselWidth:421,
			carouselHeight:218,
			frontWidth:160,
			frontHeight:218,
			hAlign:'center',
			vAlign:'center',
			autoplay:false,
			autoplayInterval:5000,
			shadow:false,
			reflection:false,
			reflectionHeight:0.2,
			reflectionOpacity:0.5,
			description:false,
			descriptionContainer:'.carousel_description'
		});
	});
	</script>
