			        }else if(pid==47){
			        	dsname = "jiningds";
			        }else if(pid==43){
			        	dsname = "zzsd";
			        }else if(pid==65){
			        	dsname = "lyds";
			        }else if(pid==48){
			        	dsname = "tads";
			        }else if(pid==50){
			        	dsname = "rzds";
			        }else if(pid==26){
			        	dsname = "qdds";
			        }else if(pid==46){
			        	dsname = "wfds";
			        }else if(pid==67){
			        	dsname = "lwds";
			        }else if(pid==42){
			        	dsname = "zbds";
			        }else if(pid==25){
			        	dsname = "jnds";
			        }else if(pid==63){
			        	dsname = "dzds";
			        }else if(pid==62){
			        	dsname = "bzds";
			        }else if(pid==44){
			        	dsname = "dyds";
			        }else if(pid==45){
			        	dsname = "ytds";
			        }else if(pid==49){
			        	dsname = "whds";
			        }else{}
			        $("#go1").attr("href","${base}/"+dsname+"/index.jhtml");
			       	$("#go1").attr("target","_blank");
			        $("#dpname").val(dsname);
    			$.ajax({  
		         type : "post",  
		          url : "${base}/getSites.jspx?pid="+pid,  
		          data : {},
		          dataType : "json" , 
		          success : function(data){
			        if(data.length>0){
			        	for(var i=0;i<data.length;i++){
			          		$("#sr_dj").append("<a target='_blank' title='"+data[i].name+"' onclick='sitesnav2("+data[i].id+");' >"+data[i].name+"</a>");
			        	}
			        }
		          }
		     });
    	}
    	function sitesnav2(pid){
    		$("#jd").html("");
    		var pids = "68,567,1406,1286,1081,1022,1042,1601,1821,1467,1353,2143,2424,1776,1845,2201,2759";
    		var dpname = $("#dpname").val();
    		$.ajax({  
		         type : "post",  
		          url : "${base}/getChannelPaths.jspx?departId="+pid,
		          data : {},
		          dataType : "json" , 
		          success : function(data){
			        $("#go2").attr("href","#");
			        if(data!=null && data!=""){
			        	if(pids.indexOf(pid)>-1){
			        		$("#go2").attr("href","${base}/"+dpname+"/index.jhtml");
			        	}else{
							$("#go2").attr("href","${base}/"+dpname+"/"+data+"/index.jhtml");
			        	}
			        	$("#go2").attr("target","_blank");
			        }else{
			        	$("#go2").attr("href","#");
			        	$("#go2").attr("target","_blank");
			        }
			        $.ajax({  
				         type : "post",  
				          url : "${base}/getSites.jspx?pid="+pid,  
				          data : {},
				          dataType : "json" , 
				          success : function(data){
					        if(data.length>0){
					        	$(".neir").css({"display":"none"});
					        	$(".jied").show();
					        	for(var i=0;i<data.length;i++){
					        		if(data[i].channelPath!=null && data[i].channelPath!=""){
					        			$("#jd").append("<a target='_blank'  href='${base}/"+dpname+"/"+data[i].channelPath+"/index.jhtml?departId="+data[i].id+"' title='"+data[i].name+"'>"+data[i].name+"</a>");
					          		}else{
					          			$("#jd").append("<a href='#' target='_blank' title='"+data[i].name+"'>"+data[i].name+"</a>");
					          		}
					        	}
					        }
				          }
				     });
				     }
		     });
    	}
