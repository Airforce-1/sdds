	            shei.onclick = function(){
	                 var shnr = document.getElementById('shnr');
	                 var hei = parseInt(getStyle(shnr,'height'));
	                 if(hei == 0){
	                   var jsons = {height:'440',right:'10'}
	                   move(shnr,jsons);
	                 }else{
	                   var jsons = {height:'0',right:'-1270'}
	                   move(shnr,jsons);
	                 }; 
	                 //console.log(hei);
	            } 
         
         
	         function getStyle(obj, name){
	            if(obj.currentStyle){
	               return obj.currentStyle[name];
	            }else{
	               return getComputedStyle(obj, false)[name];
	            }
	         }
              
	         function move(obj,json,iTagn){
	            clearInterval(obj.timer); 
	            obj.timer = setInterval(function(){  
	                for(var attr in json){
	                    var Gstyle = parseInt(getStyle(obj,attr));
	                    var speed = (json[attr] - Gstyle)/2;
	                    speed = speed>0?Math.ceil(speed):Math.floor(speed);
	                    obj.style[attr] = Gstyle + speed + 'px';
	                }
	            },30);
	     };
  	 </script>
